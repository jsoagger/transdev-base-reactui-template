  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "scripts": {
    "start": "NODE_PATH=src/:src/_components:src/pages:src/_services:src/assets:src/utils:src/store:src/_actions:src/_reducers react-scripts start",
    "build": "NODE_PATH=src/:src/_components:src/pages:src/_services:src/assets:src/utils:src/store:src/_actions:src/_reducers react-scripts build",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "NODE_PATH=src/:src/_components:src/pages:src/_services:src/assets:src/utils:src/store:src/_actions:src/_reducers react-scripts eject",
    "test": "mocha --reporter spec testSetup.js \"src/_components/Contactable/Contactable.test.js\"",
    "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.eslintignore ./template/.eslintrc ./template/.prettierrc ./template/tsconfig.json ./template/tsconfig.eslint.json",
    "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .eslintignore .eslintrc .prettierrc tsconfig.json tsconfig.eslint.json README.md README_CRA.md template/",
    "commit": "npx git-cz"
  },
  "bugs": {
    "url": "https://github.com/transdev-base-reactui-template/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  }
